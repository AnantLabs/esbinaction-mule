<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="esb.chapter9.restaurant.model.Restaurant" table="PUBLIC.RESTAURANT">
		<id name="restaurantID" column="id" type="java.lang.Long" unsaved-value="-1">
			<generator class="native" />
		</id>
		<property name="name" column="name" type="java.lang.String" />
		<property name="timeout" column="timeout" type="java.lang.Long" />
	</class>
</hibernate-mapping>